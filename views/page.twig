{% extends 'layouts/base.twig' %}

{% block content %}
	<h1>
		{{ page.title }}
	</h1>

	<ul>
		{% for item in menus.primary_navigation.get_items %}
			<li class="{{ item.classes|join(' ') }}">
				<a href="{{ item.link }}">
					{{ item.title }}
				</a>
			</li>
		{% endfor %}
	</ul>
{% endblock %}
